[pylint]
good-names = i,j,k,ex,Run,_,x,y,X,Y,x1,x2,y1,y2,g,p,xs,ys,z,u,v,tx,ty,tz,qx,qy,qz,qw,fx,fy,cx,cy,v0,v1,v2,i0,i1,i2
output-format = colorized
max-line-length = 100
extension-pkg-whitelist = PyQt5,torch,cv2,numpy
ignored-modules = numpy, PurePath
ignored-classes = numpy, PurePath
disable = fixme, 
  too-many-instance-attributes, 
  too-few-public-methods, 
  too-many-locals, 
  too-many-arguments, 
  bare-except, 
  broad-except, 
  duplicate-code